#include <stdio.h>
 
int main(){
 
  int X;
  int multiplicador = 1; 
  int resultado; 

scanf ("%d", &X);

if(X > 2 && X < 1000){
  while(multiplicador <= 10){
  resultado = multiplicador * X;
  printf("%d x %d = %d\n", multiplicador, X, resultado);
  multiplicador ++;
  }
}
return 0;
}

#include <stdio.h>
 
int main(){
    
   float N;
   float media;
   float soma = 0;
   int positivos = 0;
   

   for(i = 1; i <= 6; i++){
   scanf("%f", &N);

    if(N > 0){
      positivos++;
      soma = soma + N;
    }
   }

   media = soma/positivos;

   printf("%d valores positivos\n", positivos);
   printf("%.1f\n", media);
 
    return 0;
}
